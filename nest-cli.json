{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "root": "apps/processor-service",
  "sourceRoot": "apps/processor-service/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "apps/processor-service/tsconfig.app.json"
  },
  "monorepo": true,
  "projects": {
    "air-quality": {
      "type": "library",
      "root": "libs/air-quality",
      "entryFile": "index",
      "sourceRoot": "libs/air-quality/src",
      "compilerOptions": {
        "deleteOutDir": true,
        "tsConfigPath": "libs/air-quality/tsconfig.lib.json"
      }
    },
    "collector-service": {
      "type": "application",
      "root": "apps/collector-service",
      "entryFile": "main",
      "sourceRoot": "apps/collector-service/src",
      "compilerOptions": {
        "deleteOutDir": true,
        "tsConfigPath": "apps/collector-service/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "messaging": {
      "type": "library",
      "root": "libs/messaging",
      "entryFile": "index",
      "sourceRoot": "libs/messaging/src",
      "compilerOptions": {
        "tsConfigPath": "libs/messaging/tsconfig.lib.json"
      }
    },
    "processor-service": {
      "type": "application",
      "root": "apps/processor-service",
      "entryFile": "main",
      "sourceRoot": "apps/processor-service/src",
      "compilerOptions": {
        "deleteOutDir": true,
        "tsConfigPath": "apps/processor-service/tsconfig.app.json"
      }
    }
  }
}